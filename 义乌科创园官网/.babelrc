{
  "presets": ["react", "env","es2015"],
  "plugins": [
      ["import", {
          "libraryName": "antd",
          "libraryDirectory": "es", 
          "style": "css"
      }]
  ],
  "env": {
    "development": {
		"plugins": [
			["react-transform", {"transforms": [{"transform": "react-transform-hmr","imports": ["react"],"locals": ["module"]}]}]
		]
    }
  }
}