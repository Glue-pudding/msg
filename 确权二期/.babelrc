{
  "presets": ["react", "es2015", "env","stage-1"],
  "plugins": [
      ["import", {
          "libraryName": "antd","libraryDirectory": "es",
          "style": "css"
      }],
      ["transform-runtime"]
  ],
  "env": {
    "development": {
		"plugins": [
			["react-transform", {"transforms": [{"transform": "react-transform-hmr","imports": ["react"],"locals": ["module"]}]}]
		]
    }
  }
}